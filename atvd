void main() {
  String nome = 'geo';
  int idade = 100;
  
  if (idade <= 13 && nome == 'geo'){
    print('${nome} é criança');
  } else if (idade >13 && idade <18){
    print('${nome} é adolescente');
  } else if (idade >18 && idade <59){
    print('${nome} é adulto');
  }else if (idade >=60 && idade <=110){
    print('${nome} é idoso');
  }else{
    print('dado invalido');
  }
}
